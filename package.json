    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0",
    "sslcommerz-lts": "^1.1.0",
    "uuid": "^11.1.0"
  },
